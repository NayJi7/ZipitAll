{
    "name": "vscode-archive",
    "displayName": "%displayName%",
    "description": "%description%",
    "version": "0.9.5",
    "publisher": "YuTengjing",
    "icon": "./assets/logo.png",
    "engines": {
        "vscode": "^1.98.0"
    },
    "packageManager": "pnpm@10.13.0",
    "categories": [
        "Other"
    ],
    "keywords": [
        "archive",
        "compress",
        "decompress",
        "chrome extension",
        "firefox extension",
        "unzip",
        "zip",
        "crx",
        "vsix",
        "asar",
        "tgz",
        "gzip",
        "br",
        "tar",
        "bzip2",
        "bz2",
        "7zip",
        "7z",
        "remote"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tjx666/vscode-archive"
    },
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "VSCode Archive",
            "properties": {
                "vscode-archive.smartFlatten": {
                    "type": "boolean",
                    "default": true,
                    "description": "%config.smartFlatten.description%"
                },
                "vscode-archive.preview.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "%config.preview.enabled.description%"
                },
                "vscode-archive.preview.filePreviewEnabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "%config.preview.filePreviewEnabled.description%"
                },
                "vscode-archive.preview.maxArchiveSizeMB": {
                    "type": "number",
                    "default": 100,
                    "description": "%config.preview.maxArchiveSizeMB.description%"
                },
                "vscode-archive.preview.maxFileSizeMB": {
                    "type": "number",
                    "default": 10,
                    "description": "%config.preview.maxFileSizeMB.description%"
                }
            }
        },
        "commands": [
            {
                "command": "vscode-archive.decompress",
                "title": "%command.decompress.title%"
            },
            {
                "command": "vscode-archive.compressToZip",
                "title": "%command.compressToZip.title%"
            },
            {
                "command": "vscode-archive.compressToAsar",
                "title": "%command.compressToAsar.title%"
            },
            {
                "command": "vscode-archive.compressToGzip",
                "title": "%command.compressToGzip.title%"
            },
            {
                "command": "vscode-archive.compressToBr",
                "title": "%command.compressToBr.title%"
            },
            {
                "command": "vscode-archive.compressToTar",
                "title": "%command.compressToTar.title%"
            },
            {
                "command": "vscode-archive.compressToTgz",
                "title": "%command.compressToTgz.title%"
            },
            {
                "command": "vscode-archive.compressToVsix",
                "title": "%command.compressToVsix.title%"
            },
            {
                "command": "vscode-archive.compressToBz2",
                "title": "%command.compressToBz2.title%"
            },
            {
                "command": "vscode-archive.compressTo7z",
                "title": "%command.compressTo7z.title%"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "vscode-archive.decompress",
                    "when": "false"
                },
                {
                    "command": "vscode-archive.decompress",
                    "when": "false"
                },
                {
                    "command": "vscode-archive.compressToZip",
                    "when": "false"
                },
                {
                    "command": "vscode-archive.compressToAsar",
                    "when": "false"
                },
                {
                    "command": "vscode-archive.compressToGzip",
                    "when": "false"
                },
                {
                    "command": "vscode-archive.compressToBr",
                    "when": "false"
                },
                {
                    "command": "vscode-archive.compressToTar",
                    "when": "false"
                },
                {
                    "command": "vscode-archive.compressToTgz",
                    "when": "false"
                },
                {
                    "command": "vscode-archive.compressToVsix",
                    "when": "false"
                },
                {
                    "command": "vscode-archive.compressToBz2",
                    "when": "false"
                },
                {
                    "command": "vscode-archive.compressTo7z",
                    "when": "false"
                }
            ],
            "explorer/context": [
                {
                    "submenu": "vscode-archive.compress",
                    "group": "7_modification@0"
                },
                {
                    "command": "vscode-archive.decompress",
                    "when": "!explorerResourceIsFolder && resourceFilename =~ /.+\\.(zip|vsix|crx|asar|tgz|gzip|gz|tar|br|bz2|7z)$/i",
                    "group": "7_modification@1"
                }
            ],
            "vscode-archive.compress": [
                {
                    "command": "vscode-archive.compressToZip",
                    "group": "compress@0"
                },
                {
                    "command": "vscode-archive.compressToAsar",
                    "group": "compress@1",
                    "when": "explorerResourceIsFolder"
                },
                {
                    "command": "vscode-archive.compressToGzip",
                    "group": "compress@2",
                    "when": "!explorerResourceIsFolder"
                },
                {
                    "command": "vscode-archive.compressToBr",
                    "group": "compress@3",
                    "when": "!explorerResourceIsFolder"
                },
                {
                    "command": "vscode-archive.compressToTar",
                    "group": "compress@4"
                },
                {
                    "command": "vscode-archive.compressToTgz",
                    "group": "compress@5"
                },
                {
                    "command": "vscode-archive.compressToVsix",
                    "group": "compress@6",
                    "when": "explorerResourceIsFolder"
                },
                {
                    "command": "vscode-archive.compressToBz2",
                    "group": "compress@7",
                    "when": "!explorerResourceIsFolder"
                },
                {
                    "command": "vscode-archive.compressTo7z",
                    "group": "compress@8"
                }
            ]
        },
        "submenus": [
            {
                "id": "vscode-archive.compress",
                "label": "%submenu.compress.label%"
            }
        ],
        "customEditors": [
            {
                "viewType": "vscode-archive.archivePreview",
                "displayName": "Archive Preview",
                "selector": [
                    {
                        "filenamePattern": "*.zip"
                    },
                    {
                        "filenamePattern": "*.vsix"
                    },
                    {
                        "filenamePattern": "*.crx"
                    },
                    {
                        "filenamePattern": "*.asar"
                    },
                    {
                        "filenamePattern": "*.tgz"
                    },
                    {
                        "filenamePattern": "*.tar"
                    },
                    {
                        "filenamePattern": "*.gz"
                    },
                    {
                        "filenamePattern": "*.gzip"
                    },
                    {
                        "filenamePattern": "*.br"
                    },
                    {
                        "filenamePattern": "*.bz2"
                    },
                    {
                        "filenamePattern": "*.7z"
                    }
                ],
                "priority": "default"
            }
        ],
        "activationEvents": [
            "onCustomEditor:vscode-archive.archivePreview"
        ]
    },
    "prettier": "@yutengjing/prettier-config",
    "scripts": {
        "vscode:prepublish": "pnpm esbuild:base --minify",
        "preinstall": "npx only-allow pnpm",
        "postinstall": "stale-dep -u",
        "clean": "rimraf -rf ./out",
        "esbuild:base": "stale-dep && esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --external:original-fs --external:original-fs --format=cjs --platform=node --target=ESNext",
        "esbuild:watch": "pnpm esbuild:base --sourcemap --watch",
        "esbuild:analyze": "pnpm esbuild:base --minify --metafile=meta.json && esbuild-visualizer --metadata ./meta.json --open",
        "compile:test": "pnpm clean && tsc -p ./src/tsconfig.json && tsc -p ./test/tsconfig.json",
        "lint": "eslint src --ext ts",
        "test": "pnpm compile:test && node ./out/test/runTests.js",
        "package": "vsce package --no-dependencies",
        "release": "bumpp -c \"release: v%s\" package.json",
        "publish:vs-marketplace": "vsce publish --no-dependencies",
        "publish:open-vsx": "ovsx publish --no-dependencies"
    },
    "dependencies": {
        "@types/tar": "^7.0.87",
        "asar": "^3.2.0",
        "command-exists": "^1.2.9",
        "compressing": "^2.0.0",
        "execa": "^9.6.0",
        "jszip": "^3.10.1",
        "pretty-bytes": "^7.0.0",
        "tar": "^7.5.9"
    },
    "devDependencies": {
        "@types/command-exists": "^1.2.3",
        "@types/glob": "^9.0.0",
        "@types/mocha": "^10.0.10",
        "@types/node": "~22.18.11",
        "@types/unzipper": "^0.10.9",
        "@types/vscode": "^1.98.0",
        "@vscode/test-electron": "^2.5.2",
        "@vscode/vsce": "^3.6.0",
        "@yutengjing/eslint-config-typescript": "^2.6.1",
        "@yutengjing/prettier-config": "^2.0.0",
        "bumpp": "^10.2.0",
        "esbuild": "^0.25.6",
        "eslint": "^9.31.0",
        "glob": "^11.0.3",
        "mocha": "^11.7.1",
        "ovsx": "^0.10.5",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "stale-dep": "^0.8.2",
        "typescript": "~5.9.3"
    }
}
